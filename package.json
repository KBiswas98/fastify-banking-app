  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc && node ./build",
    "remove": "rimraf build",
    "move:templates": "cp -r src/templates/ build/templates/",
    "build:prod": "npm run remove && tsc && npm run move:templates && NODE_ENV=production pm2 start ./build",
    "build:dev": "npm run remove && tsc && npm run move:templates && NODE_ENV=production node ./build",
    "start": "node build",
    "dev": "nodemon | pino-pretty --colorize",
    "pretest": "npm run build",
    "test": "NODE_ENV=test jest --config .jest.json --coverage",
    "test:watch": "NODE_ENV=test jest --config .jest.json --watch --verbose",
    "pm2": "tsc && pm2 start ./build"
  },
